{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "max-len": 0,
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "array-callback-return": 0,
    "no-param-reassign": 0,
    "prefer-const": 0
  }
}
